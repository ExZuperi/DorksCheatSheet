{
  "manifest_version": 2,
  "name": "Dorks CheatSheet",
  "version": "1.0",
  "description": "A Firefox extension that provides list of dorks for Google, Yandex, Bing and DuckDuckGo.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
